#!/bin/bash
JOB=${1?Error: no JOBS given}
WORK=${2?Error: no WORK_PER_JOB given}
make clean
make JOBS=$JOB WORK_PER_JOB=$WORK 
export PATH="~/Documents/ENGI9875/lab2:$PATH"
echo $PATH
for i in $(seq 1000)
do
#	serial | awk '{ print $7 }' | tee -a serial_times.dat
	pthreads | awk '{ print $7 }' | tee -a pthreads_times.dat
done

for i in {3,5,10,100,1000}
do 
#	head -$i serial_times.dat | ministat -A -s
	head -$i pthreads_times.dat | ministat -A -s
done

